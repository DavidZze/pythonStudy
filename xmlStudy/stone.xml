    <job>
        <name>finish</name>
        <type>local</type>
        <configuration>
            <checkFileName>./publish_out_diff_input.json</checkFileName>
            <isCheckCount>False</isCheckCount>
        </configuration>
    </job>
    <mail-configuration>
        <receiver>liqian11@baidu.com</receiver>
    </mail-configuration>
</stone>
